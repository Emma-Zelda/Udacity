   "source": [
    "import numpy as np\n",
    "def sigma(x):\n",
    "    return 1./(1. + np.exp(-x))\n",
    "\n",
    "my_array = np.array((2.4, -1, 1.4))\n",
    "print(sigma(my_array))"
